{
    "name": "@fundtracer/server",
    "version": "1.0.0",
    "description": "FundTracer API Server - Handles auth, rate limiting, and secure API key management",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && esbuild src/functions/api.ts --bundle --platform=node --target=node18 --format=cjs --outfile=dist/functions/api.js",
        "start": "node dist/index.js",
        "dev": "tsx watch src/index.ts"
    },
    "dependencies": {
        "@fundtracer/core": "*",
        "@types/jsonwebtoken": "^9.0.10",
        "cheerio": "^1.1.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "encoding": "^0.1.13",
        "ethers": "^6.16.0",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "firebase-admin": "^12.0.0",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.3",
        "serverless-http": "^4.0.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "esbuild": "^0.21.5",
        "tsx": "^4.7.0",
        "typescript": "^5.3.0"
    }
}