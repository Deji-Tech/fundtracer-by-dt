{
  "name": "fundtracer",
  "version": "1.0.0",
  "description": "FundTracer by DT - Blockchain wallet analysis tool",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@fundtracer/web",
    "dev:server": "npm run dev --workspace=@fundtracer/server",
    "build": "npm run build --workspace=@fundtracer/core && npm run build --workspace=@fundtracer/server && npm run build --workspace=@fundtracer/web",
    "cli": "npm run start --workspace=@fundtracer/cli",
    "video": "npm run start --workspace=@fundtracer/video",
    "test": "npm run test --workspaces --if-present"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "wallet",
    "analysis",
    "fundtracer"
  ],
  "author": "DT",
  "license": "MIT",
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@tanstack/react-query": "^5.90.20",
    "esbuild": "^0.21.5",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.6.0",
    "typescript": "^5.3.0",
    "viem": "^2.45.0",
    "wagmi": "^2.19.5"
  }
}
