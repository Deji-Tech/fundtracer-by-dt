{
    "version": 2,
    "buildCommand": "npm install && npm run build --workspace=@fundtracer/web",
    "outputDirectory": "packages/web/dist",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/*.ts": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}