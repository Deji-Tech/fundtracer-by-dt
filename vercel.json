{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "packages/web/dist",
    "installCommand": "npm install",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}