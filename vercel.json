{
    "version": 2,
    "buildCommand": "npm run build --workspace=@fundtracer/web",
    "outputDirectory": "packages/web/dist",
    "installCommand": "npm install --include=dev",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}